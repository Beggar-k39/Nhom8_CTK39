@section script{
    <script>
        var ha = document.getElementById('Ha');
        var thai = document.getElementById('Thai');
        var thi = document.getElementById('Thi');
        var khanh = document.getElementById('Khanh');

        var giangvien1btn = document.getElementById('giangvien1btn');
        var giangvien2btn = document.getElementById('giangvien2btn');
        var giangvien3btn = document.getElementById('giangvien3btn');
        var giangvien4btn = document.getElementById('giangvien4btn');
        giangvien1btn.onclick = function () {
            ha.style.display = "block";
            thai.style.display = "none";
            thi.style.display = "none";
            khanh.style.display = "none";
            window.scrollTo(0, 850);
        }
        giangvien2btn.onclick = function () {
            ha.style.display = "none";
            thai.style.display = "block";
            thi.style.display = "none";
            khanh.style.display = "none";
            window.scrollTo(0, 850);
        }
        giangvien3btn.onclick = function () {
            ha.style.display = "none";
            thai.style.display = "none";
            thi.style.display = "block";
            khanh.style.display = "none";
            window.scrollTo(0, 850);
        }
        giangvien4btn.onclick = function () {
            ha.style.display = "none";
            thai.style.display = "none";
            thi.style.display = "none";
            khanh.style.display = "block";
            window.scrollTo(0, 850);
        }
    </script>
}
